{
  "name": "pranav/paraseed_media",
  "description": "Manages type of media contents and entity browsers in the site.",
  "type": "drupal-module",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Pranav Aeer",
      "email": "pranav.aeer@osseed.com"
    }
  ],
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    "assets": {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  },
  "require": {
    "composer/installers": "~1.0",
    "oomphinc/composer-installers-extender": "~1.0",
    "cweagans/composer-patches": "~1.0",
    "drupal/drupal-library-installer-plugin": "^0.3",
    "drupal/core": "^8.8.0 || ~9.0.0",
    "drupal/ds": "~3.0",
    "drupal/better_exposed_filters": "~5.0",
    "drupal/views_bulk_operations": "~3.0",
    "drupal/views_bulk_edit": "~2.0",
    "drupal/views_infinite_scroll": "~1.0",
    "drupal/entity_browser": "~2.0",
    "drupal/entity_browser_enhanced": "~1.0",
    "drupal/dropzonejs": "~2.0",
    "drupal/entity_embed": "1.1",
    "drupal/inline_entity_form": "~1.0",
    "drupal/embed": "~1.0",
    "drupal/media_entity_instagram": "~2.0",
    "drupal/media_entity_twitter": "~2.0",
    "drupal/media_library_edit": "1.0-alpha2",
    "drupal/media_revisions_ui": "~2.0",
    "drupal/crop": "~2.0",
    "drupal/focal_point": "1.4",
    "drupal/imagemagick": "~3.0",
    "drupal/imageapi_optimize": "~2.0",
    "drupal/blazy": "~2.0",
    "drupal/slick": "~2.0",
    "drupal/slick_views": "~2.0",
    "drupal/metatag": "^1.12"
  },
  "extra": {
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "installer-paths": {
      "libraries/{$name}": [
        "type:drupal-library"
      ]
    },
  }
}
